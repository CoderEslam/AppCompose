<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.doubleclick.appcompose">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompose"
        tools:targetApi="31">
        <activity
            android:name=".ListLaztColumnActivity"
            android:exported="true"
            android:label="@string/title_activity_list_lazt_column"
            android:theme="@style/Theme.AppCompose">

        </activity>
        <activity
            android:name=".LazyColumnItemsIndexedActivity"
            android:exported="true"
            android:label="@string/title_activity_list_lazt_column"
            android:theme="@style/Theme.AppCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListActivity"
            android:exported="true"
            android:label="@string/title_activity_list"
            android:theme="@style/Theme.AppCompose">

        </activity>
        <activity
            android:name=".Textfield_ButtonActivity"
            android:exported="true"
            android:label="@string/title_activity_textfield_button"
            android:theme="@style/Theme.AppCompose"></activity>
        <activity
            android:name=".StateActivity"
            android:exported="true"
            android:label="@string/title_activity_state"
            android:theme="@style/Theme.AppCompose" />
        <activity
            android:name=".StylingTextActivity"
            android:exported="true"
            android:label="@string/title_activity_styling_text"
            android:theme="@style/Theme.AppCompose" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompose" />
        <activity
            android:name=".ModifiersActivity"
            android:exported="true"
            android:label="@string/title_activity_modifiers"
            android:theme="@style/Theme.AppCompose" />
        <activity
            android:name=".ColumnActivity"
            android:exported="true"
            android:label="@string/title_activity_column"
            android:theme="@style/Theme.AppCompose" />
        <activity
            android:name=".ImageCardActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompose" />
    </application>

</manifest>